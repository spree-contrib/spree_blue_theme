/* Reset skeleton */
/* #Base 960 Grid
================================================== */

    .container                                  { position: inherit; width: auto; margin: inherit inherit; padding: inherit; }
    .column, .columns                           { float: inherit; display: inherit; margin-left: inherit; margin-right: inherit; }
    .row                                        { margin-bottom: inherit; display: inline; }

    /* Nested Column Classes */
    .column.alpha, .columns.alpha               { margin-left: inherit; }
    .column.omega, .columns.omega               { margin-right: inherit; }

    /* Base Grid */
    .container .one.column                      { width: inherit;  }
    .container .two.columns                     { width: inherit; }
    .container .three.columns                   { width: inherit; }
    .container .four.columns                    { width: inherit; }
    .container .five.columns                    { width: inherit; }
    .container .six.columns                     { width: inherit; }
    .container .seven.columns                   { width: inherit; }
    .container .eight.columns                   { width: inherit; }
    .container .nine.columns                    { width: inherit; }
    .container .ten.columns                     { width: inherit; }
    .container .eleven.columns                  { width: inherit; }
    .container .twelve.columns                  { width: inherit; }
    .container .thirteen.columns                { width: inherit; }
    .container .fourteen.columns                { width: inherit; }
    .container .fifteen.columns                 { width: inherit; }
    .container .sixteen.columns                 { width: inherit; }

    .container .one-third.column                { width: inherit; }
    .container .two-thirds.column               { width: inherit; }

    /* Offsets */
    .container .offset-by-one                   { padding-left: inherit;  }
    .container .offset-by-two                   { padding-left: inherit; }
    .container .offset-by-three                 { padding-left: inherit; }
    .container .offset-by-four                  { padding-left: inherit; }
    .container .offset-by-five                  { padding-left: inherit; }
    .container .offset-by-six                   { padding-left: inherit; }
    .container .offset-by-seven                 { padding-left: inherit; }
    .container .offset-by-eight                 { padding-left: inherit; }
    .container .offset-by-nine                  { padding-left: inherit; }
    .container .offset-by-ten                   { padding-left: inherit; }
    .container .offset-by-eleven                { padding-left: inherit; }
    .container .offset-by-twelve                { padding-left: inherit; }
    .container .offset-by-thirteen              { padding-left: inherit; }
    .container .offset-by-fourteen              { padding-left: inherit; }
    .container .offset-by-fifteen               { padding-left: inherit; }



/* #Tablet (Portrait)
================================================== */

    /* Note: Design for a width of 768px */

    @media only screen and (min-width: inherit) and (max-width: inherit) {
        .container                                  { width: inherit; }
        .container .column,
        .container .columns                         { margin-left: inherit; margin-right: inherit;  }
        .column.alpha, .columns.alpha               { margin-left: inherit; margin-right: inherit; }
        .column.omega, .columns.omega               { margin-right: inherit; margin-left: inherit; }

        .container .one.column                      { width: inherit; }
        .container .two.columns                     { width: inherit; }
        .container .three.columns                   { width: inherit; }
        .container .four.columns                    { width: inherit; }
        .container .five.columns                    { width: inherit; }
        .container .six.columns                     { width: inherit; }
        .container .seven.columns                   { width: inherit; }
        .container .eight.columns                   { width: inherit; }
        .container .nine.columns                    { width: inherit; }
        .container .ten.columns                     { width: inherit; }
        .container .eleven.columns                  { width: inherit; }
        .container .twelve.columns                  { width: inherit; }
        .container .thirteen.columns                { width: inherit; }
        .container .fourteen.columns                { width: inherit; }
        .container .fifteen.columns                 { width: inherit; }
        .container .sixteen.columns                 { width: inherit; }

        .container .one-third.column                { width: inherit; }
        .container .two-thirds.column               { width: inherit; }

        /* Offsets */
        .container .offset-by-one                   { padding-left: inherit; }
        .container .offset-by-two                   { padding-left: inherit; }
        .container .offset-by-three                 { padding-left: inherit; }
        .container .offset-by-four                  { padding-left: inherit; }
        .container .offset-by-five                  { padding-left: inherit; }
        .container .offset-by-six                   { padding-left: inherit; }
        .container .offset-by-seven                 { padding-left: inherit; }
        .container .offset-by-eight                 { padding-left: inherit; }
        .container .offset-by-nine                  { padding-left: inherit; }
        .container .offset-by-ten                   { padding-left: inherit; }
        .container .offset-by-eleven                { padding-left: inherit; }
        .container .offset-by-twelve                { padding-left: inherit; }
        .container .offset-by-thirteen              { padding-left: inherit; }
        .container .offset-by-fourteen              { padding-left: inherit; }
        .container .offset-by-fifteen               { padding-left: inherit; }
    }


/*  #Mobile (Portrait)
================================================== */

    /* Note: Design for a width of 320px */

    @media only screen and (max-width: inherit) {
        .container { width: inherit; }
        .columns, .column { margin: inherit; }

        .container .one.column,
        .container .two.columns,
        .container .three.columns,
        .container .four.columns,
        .container .five.columns,
        .container .six.columns,
        .container .seven.columns,
        .container .eight.columns,
        .container .nine.columns,
        .container .ten.columns,
        .container .eleven.columns,
        .container .twelve.columns,
        .container .thirteen.columns,
        .container .fourteen.columns,
        .container .fifteen.columns,
        .container .sixteen.columns,
        .container .one-third.column,
        .container .two-thirds.column  { width: inherit; }

        /* Offsets */
        .container .offset-by-one,
        .container .offset-by-two,
        .container .offset-by-three,
        .container .offset-by-four,
        .container .offset-by-five,
        .container .offset-by-six,
        .container .offset-by-seven,
        .container .offset-by-eight,
        .container .offset-by-nine,
        .container .offset-by-ten,
        .container .offset-by-eleven,
        .container .offset-by-twelve,
        .container .offset-by-thirteen,
        .container .offset-by-fourteen,
        .container .offset-by-fifteen { padding-left: inherit; }

    }


/* #Mobile (Landscape)
================================================== */

    /* Note: Design for a width of 480px */

    @media only screen and (min-width: inherit) and (max-width: inherit) {
        .container { width: inherit; }
        .columns, .column { margin: inherit; }

        .container .one.column,
        .container .two.columns,
        .container .three.columns,
        .container .four.columns,
        .container .five.columns,
        .container .six.columns,
        .container .seven.columns,
        .container .eight.columns,
        .container .nine.columns,
        .container .ten.columns,
        .container .eleven.columns,
        .container .twelve.columns,
        .container .thirteen.columns,
        .container .fourteen.columns,
        .container .fifteen.columns,
        .container .sixteen.columns,
        .container .one-third.column,
        .container .two-thirds.column { width: inherit; }
    }


